{
  "name": "@mdap/cli",
  "version": "0.1.0",
  "description": "MDAP CLI - Command line tools for MDAP SDK",
  "type": "module",
  "bin": {
    "mdap": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/cli.ts src/index.ts --format esm --dts --clean",
    "dev": "tsup src/cli.ts src/index.ts --format esm --dts --watch",
    "typecheck": "pnpm exec tsc -b ../core && pnpm exec tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mdap",
    "cli",
    "llm",
    "ai",
    "agents"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mdap/core": "workspace:*",
    "@mdap/adapters": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
