{
  "name": "mdap-sdk",
  "version": "0.0.0",
  "private": true,
  "description": "Massively Decomposed Agentic Processes - Make your AI agents reliable",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/mdap-sdk"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "test": "pnpm -r test",
    "lint": "eslint packages/*/src --ext .ts",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish",
    "simulate": "npx tsx examples/simulation/code-analysis-1000.ts",
    "simulate:dry": "npx tsx examples/simulation/code-analysis-1000.ts --dry-run"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "@eslint/js": "^9.39.1",
    "@types/node": "^20.10.0",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.48.0",
    "vitest": "^1.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0"
  }
}
